    "jsdom": "^11.5.1",
    "jsdom-global": "^3.0.2",
    "laravel-mix": "^1.0",
    "lodash": "^4.17.4",
    "mocha": "^4.0.1",
    "mocha-webpack": "^1.0.1",
    "vue": "^2.5.9",
    "vue-loader": "^13.5.0",
    "vue-template-compiler": "^2.5.9",
    "vue-test-utils": "^1.0.0-beta.6",
    "webpack": "^3.9.1"
  },
